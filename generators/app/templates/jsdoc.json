{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": "(js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "exclude": [],
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "package": "./package.json",
    "readme": "./README.md",
    "template": "node_modules/docdash",
    "destination": "./docs/jsdoc/",
    "recurse": false
  }
}
